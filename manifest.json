{
  "name": "SUEY",
  "description": "LiHKG plugin for suicide's reply",
  "version": "1.2",
  "manifest_version": 3,
  "action": {
    "default_icon": "catReply_512x512.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://lihkg.com/*"],
      "js": ["assets/index.js"]
    }
  ],
  "icons": {
    "16": "catReply_16.png",
    "48": "catReply_48.png",
    "128": "catReply_128.png"
  }
}
